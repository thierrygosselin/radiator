url = "https://thierrygosselin.github.io/radiator/",
doi = "10.5281/zenodo.3687060",
textVersion = paste("Thierry Gosselin, Manuel Lamothe and Peter Grewe (2020). radiator: RADseq Data Exploration, Manipulation and Visualization using R. R package version 1.1.4 https://thierrygosselin.github.io/radiator/. doi : 10.5281/zenodo.3687060")
)
